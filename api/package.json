{
  "name": "api",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "engines": {
    "node": "15.x",
    "npm": "7.x",
    "yarn": "1.x"
  },
  "scripts": {
    "prisma:reset": "yarn prisma migrate reset --force",
    "prisma:dev": "yarn prisma migrate dev --name init",
    "heroku-postbuild": "yarn prisma:reset && yarn prisma:dev && tsc",
    "watch": "tsc -w",
    "dev": "nodemon dist/index.js",
    "dev:start": "node dist/index.js",
    "start": "node dist/index.ts",
    "publish-heroku": "cd ../ && git add . && git commit -m 'heroku' && git subtree push --prefix api heroku main"
  },
  "devDependencies": {
    "@types/express": "^4.17.11",
    "@types/node": "^15.0.2",
    "@types/passport": "^1.0.6",
    "@types/passport-local": "^1.0.33",
    "nodemon": "^2.0.7",
    "prisma": "^2.22.1",
    "ts-node": "^9.1.1",
    "typegraphql-prisma": "^0.14.1",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "@prisma/client": "^2.22.1",
    "@types/graphql-fields": "^1.3.3",
    "apollo-server-express": "^2.24.0",
    "bcryptjs": "^2.4.3",
    "class-validator": "^0.13.1",
    "express": "^4.17.1",
    "graphql": "^15.5.0",
    "graphql-fields": "^2.0.3",
    "graphql-scalars": "^1.9.3",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "type-graphql": "^1.1.1"
  }
}

{
  "name": "api",
  "version": "1.0.0",
  "main": "main.js",
  "license": "MIT",
  "scripts": {
    "prisma:reset": "yarn prisma migrate reset --force",
    "prisma:dev": "yarn prisma migrate dev",
    "prisma:deploy": "yarn prisma migrate deploy",
    "watch": "tsc -w",
    "dev:tsnd": "ts-node-dev --transpile-only src/main.ts",
    "dev": "tsc -w & nodemon -q -w dist dist/main.js",
    "dev:start": "node dist/main.js",
    "start": "tsc & node dist/main.js",
    "heroku-postbuild": "yarn prisma:deploy && tsc"
  },
  "devDependencies": {
    "@types/express": "^4.17.11",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/multer": "^1.4.5",
    "@types/node": "^15.0.2",
    "@types/passport": "^1.0.6",
    "@types/passport-jwt": "^3.0.5",
    "@types/passport-local": "^1.0.33",
    "nodemon": "^2.0.7",
    "prisma": "2.23.0",
    "ts-node": "^9.1.1",
    "ts-node-dev": "^1.1.6",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "@prisma/client": "2.23.0",
    "@types/graphql-fields": "^1.3.3",
    "@types/multer-s3": "^2.7.9",
    "apollo-server-express": "^2.24.0",
    "aws-sdk": "^2.926.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "date-fns": "^2.22.1",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "graphql": "^15.5.0",
    "graphql-fields": "^2.0.3",
    "graphql-scalars": "^1.9.3",
    "jsonwebtoken": "^8.5.1",
    "multer": "^1.4.2",
    "multer-s3": "^2.9.0",
    "nexus": "^1.0.0",
    "nexus-plugin-prisma": "^0.35.0",
    "nexus-shield": "^2.0.0",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "reflect-metadata": "^0.1.13"
  }
}
